alias gd="git diff"  
alias be='bundle exec'  
PATH=$PATH:$HOME/.rvm/bin # Add RVM to PATH for scripting  
[[ -s "$HOME/.rvm/scripts/rvm"  ]] && . "$HOME/.rvm/scripts/rvm"  # This loads RVM  